int findDuplicate(vector<int> &arr) 
{
  //Approch 1
    int ans=0;
    for(int i=0;i<arr.size();i++)
    ans=ans^arr[i];
    for(int i=0;i<arr.size();i++)
    ans=ans^i;
    return ans;
  //Approch 2
    // int n=arr.size();
    // int temp[n]={0};
    // for(int i=0;i<n;i++)
    // {
    //     if(temp[arr[i]]!=0)
    //     return arr[i];
    //     temp[arr[i]]++;
    // }	
}
